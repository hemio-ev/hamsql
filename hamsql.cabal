name:                hamsql
version:             0.0.1.0
synopsis:            HamSql
-- description:         
homepage:            https://github.com/qua-bla/hamsql
license:             GPL-3
license-file:        LICENSE
author:              Michael Herold et al.
maintainer:          quabla@hemio.de
-- copyright:           
-- category:            
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable hamsql
  main-is:             Main.hs
